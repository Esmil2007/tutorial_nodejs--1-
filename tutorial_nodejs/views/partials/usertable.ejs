<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
      <th>Senha</th>
    </tr>
  </thead>
  <tbody>
    <% let i=0; for (i = 0 ;i<dados.length; i++) { %>
    <tr>
      <td><%= dados[i].id %></td>
      <td><%= dados[i].username%> </td>
      <td><%= dados[i].password%> </td>
    </tr>
    <% } %>
    
</tbody>
<tfoot>
    <tr><th colspan="3">
        <%- `Qtde de registros: ${dados.length}` %>
    </th>
    </tr>
    </tfoot>

</table>